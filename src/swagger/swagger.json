  "info": {
    "title": "Virgool API",
    "description": "NestJS Virgool API description",
    "version": "0.0.1",
    "contact": {}
  },
  "tags": [],
  "servers": [
    {
      "url": "http://localhost:3500"
    }
  ],
  "components": {
    "securitySchemes": {
      "Authorization": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http",
        "in": "header"
      },
      "cookieAuth": {
        "type": "apiKey",
        "in": "cookie",
        "name": "access_token",
        "description": "Access token (Stored in cookie for secure access)"
      }
    },
    "schemas": {
      "ProfileDto": {
        "type": "object",
        "properties": {
          "nick_name": {
            "type": "string",
            "example": "john_doe"
          },
          "age": {
            "type": "number",
            "example": 25,
            "nullable": true
          },
          "bio": {
            "type": "string",
            "example": "Backend developer",
            "nullable": true
          },
          "image_profile": {
            "type": "string",
            "example": "https://example.com/profile.jpg",
            "nullable": true,
            "format": "binary"
          },
          "bg_image": {
            "type": "string",
            "example": "https://example.com/bg.jpg",
            "nullable": true
          },
          "birthday": {
            "format": "date-time",
            "type": "string",
            "example": "1998-05-12",
            "nullable": true,
            "description": "ISO date string"
          },
          "linkdin_profile": {
            "type": "string",
            "example": "https://linkedin.com/in/username",
            "nullable": true
          },
          "gender": {
            "type": "string",
            "enum": [
              "male",
              "female",
              "other"
            ],
            "nullable": true
          }
        }
      },
      "UpdateUserDto": {
        "type": "object",
        "properties": {}
      },
      "ChangeEmailDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          }
        },
        "required": [
          "email"
        ]
      },
      "AuthType": {
        "type": "string",
        "enum": [
          "login",
          "register"
        ]
      },
      "AuthMethod": {
        "type": "string",
        "enum": [
          "username",
          "phone",
          "email"
        ]
      },
      "AuthDto": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string"
          },
          "type": {
            "allOf": [
              {
                "$ref": "#/components/schemas/AuthType"
              }
            ]
          },
          "method": {
            "allOf": [
              {
                "$ref": "#/components/schemas/AuthMethod"
              }
            ]
          }
        },
        "required": [
          "username",
          "type",
          "method"
        ]
      },
      "CheckOtpDto": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string"
          }
        },
        "required": [
          "code"
        ]
      },
      "CreateCategoryDto": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "priority": {
            "type": "number"
          }
        },
        "required": [
          "title"
        ]
      },
      "UpdateCategoryDto": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "priority": {
            "type": "number"
          }
        }
      }
    }
  }
